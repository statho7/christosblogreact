{
    "routes": [
      {
        "route": "/"
      },
      {
        "route": "/category/*"
      },
      {
        "route": "/article/*"
      },
      {
        "route": "/api/*",
        "methods": ["GET"]
      }
    ],
    "navigationFallback": {
      "rewrite": "index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "responseOverrides": {
      "400": {
        "rewrite": "/index.html"
      },
      "401": {
        "redirect": "/index.html"
      },
      "403": {
        "rewrite": "/index.html"
      },
      "404": {
        "rewrite": "/index.html"
      }
    },
    "globalHeaders": {
      "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'; img-src data:;"
    },
    "mimeTypes": {
      ".json": "text/json"
    }
  }